<!DOCTYPE html>
<html lang="en">
        <%- include ../partials/head.ejs %>
        <link rel="stylesheet" href="/stylesheets/seriesoverview.css">
        <title>Het Serie Spel</title>
    </head>
    <body id="serie-overview">
        <%- include ../partials/header.ejs %>
        <h1>Jouw persoonlijke top 5</h1>
        <% for (var i = 0; i < results.length; i++) { %>
            <a href="/review/<%- results[i].name%>">
                <label>
                    <div>
                        <img src="<%- results[i].data.review.imgURL%>" alt="<%- results[i].name%>">
                    </div>
                    <h2><span><%- results[i].name + ' ' + results[i].matchAll + '%'%></span></h2>
                </label>
            </a>
        <% } %>
    </body>
</html>
